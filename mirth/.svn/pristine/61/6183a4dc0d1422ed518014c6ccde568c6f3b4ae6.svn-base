package com.misyshealthcare.connect.doc.ccd;

import java.util.Calendar;

/**
 * The range of effective time for CDA document
 *
 * @author Wenzhi Li
 * @version 2.0, Jul 13, 2006
 */
public class EffectiveTime {
    Calendar start = null;
    Calendar end = null;

    public EffectiveTime(Calendar start, Calendar end) {
        this.start = start;
        this.end = end;
    }

    public Calendar getStart() {
        return this.start;
    }

    public Calendar getEnd() {
        return this.end;
    }

}
