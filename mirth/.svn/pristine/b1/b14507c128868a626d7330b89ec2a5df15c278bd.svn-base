<?xml version="1.0" encoding="UTF-8"?>

<!-- The PIX Manager Actor Connections -->
<Configuration>
	<standardconnection name="init-pix-manager">
		<hostname>67.155.0.245</hostname>
		<port>2200</port>
		<Identifier name="ReceivingApplication">
			<NamespaceId>REMOTE_APPLICATION</NamespaceId>
		</Identifier>
		<Identifier name="ReceivingFacility">
			<NamespaceId>REMOTE_FACILITY</NamespaceId>
		</Identifier>
	
		<Identifier name="MisysAssigningAuthority">
			<NamespaceId>LocalAsigningAuthority</NamespaceId>
			<UniversalId>1.3.6.1.4.1.21367.x.x.x.x</UniversalId>
			<UniversalIdType>ISO or IT-UT</UniversalIdType>
		</Identifier>
		
		<Identifier name="AssigningAuthority">
			<NamespaceId>MIEH</NamespaceId>
			<UniversalId>1.3.6.1.4.1.21367.y.y.y.y</UniversalId>
			<UniversalIdType>ISO</UniversalIdType>
		</Identifier>
			
		<Identifier name="SendingApplication">
			<NamespaceId>LOCAL_APPLICATION</NamespaceId>
		</Identifier>

		<Identifier name="SendingFacility">
			<NamespaceId>LOCAL_FACILITY</NamespaceId>
		</Identifier>
	
		<!-- Boiler Plate -->
        <PropertySet name="PixQueryName">
			<Entry name="identifier" value="QRY_1001"/>
			<Entry name="text" value="Query for Corresponding Identifiers"/>
			<Entry name="codingSystem" value="IHEDEMO"/>
		</PropertySet>
	</standardconnection>

</Configuration>