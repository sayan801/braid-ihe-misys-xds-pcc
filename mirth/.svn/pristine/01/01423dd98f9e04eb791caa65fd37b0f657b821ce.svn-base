<connectorMetaData>
  <name>IHE Sender</name>
  <author>WebReach, Inc.</author>
  <pluginVersion>@mirthversion</pluginVersion>
  <mirthVersion>1.7.0</mirthVersion>
  <updateUrl><![CDATA[http://extensions.mirthproject.org/updates/?mirthversion=${mirthVersion}&version=${version}&name=${name}&serverid=${serverid}]]></updateUrl>
  <url>http://extensions.mirthproject.org</url>
  <description>This connector allows Mirth to connect to an IHE instance.</description>
  <clientClassName>com.webreach.mirth.connectors.ihe.IheSender</clientClassName>
  <serverClassName>com.webreach.mirth.connectors.ihe.IheConnector</serverClassName>
  <sharedClassName>com.webreach.mirth.connectors.ihe.IheSenderProperties</sharedClassName>
  <transformers></transformers>
  <protocol>ihe</protocol>
  <type>SENDER</type>
  <enabled>true</enabled>
  <mule-properties>
      <property name="connector">com.webreach.mirth.connectors.ihe.IheConnector</property>
      <property name="dispatcher.factory">com.webreach.mirth.connectors.ihe.IheMessageDispatcherFactory</property>
      <property name="message.receiver">com.webreach.mirth.connectors.ihe.IheMessageReceiver</property>
      <property name="message.adapter">com.webreach.mirth.connectors.ihe.IheMessageAdapter</property>
  </mule-properties>
</connectorMetaData>