<!-- The Patient Feed with Global ID Actor Connections -->
<!-- For Connectathon 2007 maybe... I think that these are just the pix managers with the global misys codes -->

<Configuration>
	<standardconnection name="accenx-global-feed">
		<hostname>64.78.149.228</hostname>
		<port>7777</port>
		<Identifier name="ReceivingApplication">
			<NamespaceId>ACCENX</NamespaceId>
		</Identifier>
		<Identifier name="ReceivingFacility">
			<NamespaceId>ACCENX</NamespaceId>
		</Identifier>
		<includefile name="GlobalMisys.xml" />
	</standardconnection>
	
