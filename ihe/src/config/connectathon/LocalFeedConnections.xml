<!-- The Patient Feed with Local ID Actor Connections -->
<!-- For Connectathon 2008 maybe... I think that these are just the pix managers with the local misys codes -->

<Configuration>
	<standardconnection name="mesa-local-feed">
		<hostname>localhost</hostname>
		<port>3600</port>
		<Identifier name="ReceivingApplication">
			<NamespaceId>MESA_SOFTWARE</NamespaceId>
		</Identifier>
		<Identifier name="ReceivingFacility">
			<NamespaceId>MESA</NamespaceId>
		</Identifier>
		<includefile name="LocalMisys.xml" />
		<Identifier name="TestA">
			<NamespaceId>HIMSS2005</NamespaceId>
			<UniversalId></UniversalId>
			<UniversalIdType></UniversalIdType>
		</Identifier>
		<Identifier name="TestB">
			<NamespaceId>HIMSS2005</NamespaceId>
			<UniversalId>1.3.6.1.4.1.21367.2005.1.1</UniversalId>
			<UniversalIdType>ISO</UniversalIdType>
		</Identifier>
		<Identifier name="TestC">
			<NamespaceId></NamespaceId>
			<UniversalId>1.3.6.1.4.1.21367.2005.1.1</UniversalId>
			<UniversalIdType>ISO</UniversalIdType>
		</Identifier>
	</standardconnection>

