<!-- The XDS Repository Actor for Documentr Retrieving Connections -->
<!-- For Connectathon 2008  COMPLETE -->

<Configuration>
	<standardconnection name="nist-xdsb-retrieve">
		<hostname>ihexds.nist.gov</hostname>
		<port>9080</port>
        <urlPath>/tf5/services/xdsrepositoryb</urlPath>
	</standardconnection>
    <secureconnection name="nist-xdsb-retrieve-secure">
		<hostname>ihexds.nist.gov</hostname>
        <port>9085</port>
        <urlPath>/tf5/services/xdsrepositoryb</urlPath>
        <KEYSTORE>/certs/EHR_MISYS_08_KEY.p12</KEYSTORE>
        <KEYPASS>password</KEYPASS>
        <TRUSTSTORE>/certs/EHR_MISYS_08_TRUST.jks</TRUSTSTORE>
        <TRUSTPASS>password</TRUSTPASS>
	</secureconnection>		
	
	<standardconnection name="hxti-xdsb-retrieve">
